root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 4
indent_style = space
max_line_length = 120
trim_trailing_whitespace = true

[*.java]
indent_size = 4
indent_style = space

[*.xml]
indent_size = 4
indent_style = space

[*.yml]
indent_size = 2
indent_style = space

[*.md]
indent_size = 2
indent_style = space
trim_trailing_whitespace = false
